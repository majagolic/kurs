package objekatTelefonskiImenik;

import java.time.LocalDate;

public class Kontakt {
	
	private int id;
	private String ime = "";
	private String prezime = "";
	private String radnoMesto = "";
	private String prostorija = "";
	private int lokal;
	private LocalDate datum = LocalDate.now();
	
	
	public Kontakt() {
		
	}


	public Kontakt(int id, String ime, String prezime, String radnoMesto, String prostorija, int lokal,
			LocalDate datum) {
	
		this.id = id;
		this.ime = ime;
		this.prezime = prezime;
		this.radnoMesto = radnoMesto;
		this.prostorija = prostorija;
		this.lokal = lokal;
		this.datum = datum;
	}


	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}


	public String getIme() {
		return ime;
	}


	public void setIme(String ime) {
		this.ime = ime;
	}


	public String getPrezime() {
		return prezime;
	}


	public void setPrezime(String prezime) {
		this.prezime = prezime;
	}


	public String getRadnoMesto() {
		return radnoMesto;
	}


	public void setRadnoMesto(String radnoMesto) {
		this.radnoMesto = radnoMesto;
	}


	public String getProstorija() {
		return prostorija;
	}


	public void setProstorija(String prostorija) {
		this.prostorija = prostorija;
	}


	public int getLokal() {
		return lokal;
	}


	public void setLokal(int lokal) {
		this.lokal = lokal;
	}


	public LocalDate getDatum() {
		return datum;
	}


	public void setDatum(LocalDate datum) {
		this.datum = datum;
	}


	@Override
	public String toString() {
		return "Kontakt [id=" + id + ", ime=" + ime + ", prezime=" + prezime + ", radnoMesto=" + radnoMesto
				+ ", prostorija=" + prostorija + ", lokal=" + lokal + ", datum=" + datum + "]";
	}
	
	
	
	
	
	

}
